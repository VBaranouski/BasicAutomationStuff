<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >

<suite name="Suite_One" parallel="methods" thread-count="3">

    <parameter name="city" value="New York"/>
    <parameter name="adults"  value="2"/>

    <listeners>
        <listener class-name="com.expedia.automation.listeners.SuiteListener" />
        <listener class-name="com.expedia.automation.listeners.TestListener" />
        <listener class-name="com.expedia.automation.listeners.AnnotationTransformer" />
    </listeners>

<test name="ExpediaFull" >
        <groups>
            <run>
                <include name="full"/>
            </run>
        </groups>

        <classes>
            <class name="com.expedia.automation.tests.Testing" />

        </classes>
    </test>
</suite>



