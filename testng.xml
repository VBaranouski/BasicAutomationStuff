<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >

<suite name="Suite_One" verbose="1" parallel="tests" thread-count="5">

    <parameter name="city" value="New York"/>
    <parameter name="adults"  value="2"/>

    <listeners>
        <listener class-name="com.expedia.automation.listener.SuiteListener" />
        <listener class-name="com.expedia.automation.listener.TestListener" />
        <listener class-name="com.expedia.automation.listener.AnnotationTransformer" />
    </listeners>

<test name="ExpediaFull" >
        <groups>
            <run>
                <include name="full"/>
            </run>
        </groups>

        <classes>
            <class name="com.expedia.automation.tests.Testing" />

        </classes>
    </test>
</suite>



